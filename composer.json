{
    "name": "thedepart3d/invoicr-for-laravel",
    "description": "Generate Invoices on the go using Invoicr default templates.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Thedepart3d\\InvoicrForLaravel\\": "src"
        }
    },
    "authors": [
        {
            "name": "thedepart3d",
            "email": "thedepart3d@protonmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
      "php": "^8.1",
      "illuminate/console": "^8|^9|^10",
      "illuminate/support": "^8|^9|^10"
    },
    "config": {
      "optimize-autoloader": true,
      "sort-packages": true
    },
    "extra": {
      "laravel": {
        "providers": [
          "Thedepart3d\\InvoicrForLaravel\\InvoiceServiceProvider"
        ]
      }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=invoicr-config --ansi --force"
        ]
    },
    "version": "0.0.1"
}
